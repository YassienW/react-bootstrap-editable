{
  "name": "react-bootstrap-editable",
  "keywords": [
    "react",
    "bootstrap",
    "editable",
    "xeditable",
    "edit",
    "ajax"
  ],
  "version": "0.4.1",
  "main": "dist/editable.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6969 -c .storybook",
    "babel": "babel src -d scripts",
    "browserify": "browserify ./scripts/Editable.js -s Editable -g uglifyify | uglifyjs --compress --mangle > ./dist/editable.js",
    "dist": "npm run babel && npm run browserify",
    "distdocs": "build-storybook -c .storybook -o docs"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "author": "Yassien",
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reactstrap": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.9",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^5.0.9",
    "@storybook/addons": "^5.0.9",
    "@storybook/react": "^5.1.0-alpha.29",
    "babel-loader": "^8.0.5",
    "browserify": "^16.2.3",
    "uglify-js": "^3.5.4",
    "uglifyify": "^5.0.1"
  },
  "description": "Clean editable bootstrap components for react. Ajax support!",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YassienW/react-bootstrap-editable.git"
  },
  "bugs": {
