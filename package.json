{
  "name": "react-bootstrap-editable",
  "version": "0.2.1",
  "main": "dist/editable.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6969 -c .storybook",
    "babel": "babel src -d scripts",
    "browserify": "npm run babel && browserify ./scripts/Editable.js -s Editable -g [envify --NODE_ENV production] -g uglifyify | uglifyjs --compress --mangle > ./dist/editable.js",
    "dist": "npm run babel && npm run browserify && build-storybook -c .storybook -o docs"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "author": "Yassien",
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "reactstrap": "^7.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "babel-loader": "^8.0.5",
    "browserify": "^16.2.3",
    "envify": "^4.1.0",
